package exo2;

import java.util.List;

public class Point {

	private int x, y ;

	public Point (int x, int y) { 
		this.x = x ; this.y = y ;
	}

	public int getX() { 
		return x ; 
	}

	public int getY() { 
		return y ; 
	}

	public void affiche (){
		System.out.print(" [ " + x + ", " + y + "] ") ;
	}

	public static void traiteListe(List<Point> liste) {

	}
}
