package exo3;

public class PointStream {

	private int x, y ;

	public PointStream (int x, int y) {
		this.x = x ; 
		this.y = y ; 
	}

	public int getX() { 
		return x ; 
	}

	public int getY() { 
		return y ; 
	}



	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
